{
  "name": "Iconfinder Dashboard",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Iconfinder Dashboard",
  "homepage_url": "http://iconfinder.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "chrome://favicon/",
    "tabs",
    "activeTab",
    "http://iconfider.com/*",
    "https://iconfider.com/*",
    "https://www.iconfinder.com/json/search/?q=user&c=10&p=0&min=1&max=48&api_key=94d921f8453d416da13bba8deb66112c/*",
    "http://fonts.googleapis.com/*",
    "https://*/",
    "http://*/"
  ],
  "omnibox": {
    "keyword": "icon"
  }
}